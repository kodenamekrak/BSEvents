{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "BSEvents",
    "id": "bs-events",
    "version": "1.0.0",
    "url": "https://github.com/kodenamekrak/BSEvents",
    "additionalData": {
      "overrideSoName": "libbs-events.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "logcat": [
        "pwsh ./scripts/start-logging.ps1 -self"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 -clean",
        "qpm qmod manifest",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ]
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.0.0",
      "additionalData": {
       
      }
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3500.0.0",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.3",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.6.3",
      "additionalData": {}
    }
  ]
}